{
    "parser":"@typescript-eslint/parser",    
    "extends":[
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended" 
    ],
    "plugins":[
       "@typescript-eslint",
       "prettier",
       "react"
    ],
    "env":{
       "browser":true,
       "node":true
    },
    "parserOptions":{
       "ecmaVersion":6
    },
    "settings":{
       "react":{
          "version":"latest"
       },
       "import/resolver":{
          "alias":{
             "extensions":[
                ".js",
                ".ts",
                ".tsx"
             ]
          }
       }
    }
 }